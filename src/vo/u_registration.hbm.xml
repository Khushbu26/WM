<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="vo.URegistrationVo" table="User_reg">

		<id name="uRegId" type="int" column="user_Id">
			<generator class="increment" />
		</id>

		<property name="name" >
			<column name="name" />
		</property>

		<property name="number" type="long">
			<column name="number"  />
		</property>
		
		<property name="email">
			<column name="email" />
		</property>
		
		<property type="int" name="pin">
			<column name="pin" />
		</property>
		
		<property name="password">
			<column name="password" />
		</property>
		
		<property name="city">
			<column name="city" />
		</property>
		
		
		
		
	</class>

</hibernate-mapping>
